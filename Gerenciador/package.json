{
  "name": "sistema_login",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "make": "electron-forge make",
    "build": "electron-builder",
    "package": "electron-forge package"
  },
  "keywords": [],
  "author": "Aparecida Lopes",
  "license": "ISC",
  "description": "sistema_login",
  "dependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "electron-forge": "^5.2.4",
    "electronmon": "^2.0.4"
  },
  "devDependencies": {
    "electron": "^39.1.2",
    "electron-builder": "^26.0.12"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "SistemaLogin"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "SistemaLogin"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ]
        }
      ]
    }
  },
  "build": {
    "appId": "Sistema Login",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "target": "dmg"
    }
  }
}
